/ {
	pcb {
		version-str = "v0.2";
		version-num = <2>;
	};

	i-load {
		compatible = "adc-gain";
		io-channels = <&adc1 5>;
		// amp gain: 68/2.2, resistor: 2 mOhm
		numerator = <1000>;
		denominator = <62>;	// 2*68/2.2 = 61.8181
	};
};
